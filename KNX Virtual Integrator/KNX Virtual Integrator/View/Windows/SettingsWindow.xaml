                                BorderThickness="2" Padding="-1,-1,-1,-1">

                        <TabItem x:Name="OngletParametresApplication" Style="{StaticResource LightOnglet}">
                            <ScrollViewer x:Name="GeneralSettingsTab" VerticalScrollBarVisibility="Auto"
