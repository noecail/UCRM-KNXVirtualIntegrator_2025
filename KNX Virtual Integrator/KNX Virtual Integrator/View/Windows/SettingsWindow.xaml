<Window x:Class="KNX_Virtual_Integrator.View.Windows.SettingsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Height="705" Width="500"
        ResizeMode="NoResize"
        WindowStyle="None"
        Title="Paramètres"
        AllowsTransparency="True"
        Background="{StaticResource OffWhiteBackgroundBrush}"
        BorderThickness="1.5"
        BorderBrush="{StaticResource GrayBorderBrush}"
        Closing="ClosingSettingsWindow"
        PreviewKeyDown="SettingsWindow_PreviewKeyDown">

    <Window.Resources>

        <!-- ************************ Style pour le bouton de fermeture de la fenêtre ************************ -->
        <!-- Le bouton de fermeture de la fenêtre doit être stylisé car la fenêtre est mise en WindowStyle="None" -->
        <Style x:Key="CloseButtonStyle" TargetType="Button" BasedOn="{StaticResource BaseButtonStyle}">
            <Setter Property="Background" Value="{StaticResource RedBrush}" />
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="Button">
                        <Border Background="{TemplateBinding Background}"
                                BorderThickness="{TemplateBinding BorderThickness}">
                            <Image Width="16" Height="16" Margin="5,0,5,0" VerticalAlignment="Center"
                                   HorizontalAlignment="Center">
                                <Image.Source>
                                    <DrawingImage>
                                        <DrawingImage.Drawing>
                                            <DrawingGroup ClipGeometry="M0,0 V200 H200 V0 H0 Z">
                                                <DrawingGroup Opacity="1">
                                                    <DrawingGroup Opacity="1">
                                                        <GeometryDrawing Brush="{StaticResource WhiteBrush}"
                                                                         Geometry="F1 M200,200z M0,0z M99.63,120.8C72.17,148.17 46.48,173.78 20.39,199.78 13.61,192.76 7.41,186.34 0,178.66 25.77,153.17 51.75,127.46 77.55,101.93 51.64,76.29 25.89,50.81 0.71,25.89 8.13,18.48 14.31,12.29 20.73,5.88 45.36,30.49 71.13,56.23 96.6,81.68 124.58,53.7 151.26,27.02 178.29,0 186.31,8.18 192.53,14.53 200,22.15 173.32,48.58 146.55,75.1 119.98,101.43 146.76,127.82 173.39,154.06 199.66,179.93 191.94,187.46 185.67,193.59 179.11,200 153.67,174.65 127.03,148.11 99.63,120.8z" />
                                                    </DrawingGroup>
                                                </DrawingGroup>
                                            </DrawingGroup>
                                        </DrawingImage.Drawing>
                                    </DrawingImage>
                                </Image.Source>
                            </Image>
                        </Border>
                        <ControlTemplate.Triggers>
                            <Trigger Property="IsMouseOver" Value="True">
                                <Setter Property="Background" Value="{StaticResource RedAccentBrush}" />
                            </Trigger>
                        </ControlTemplate.Triggers>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>


        <!-- ************************ Styles pour les boutons en bas de la fenêtre ************************ -->
        <Style x:Key="BottomButtonLight" TargetType="Button" BasedOn="{StaticResource BaseButtonStyle}">
            <Setter Property="Background" Value="{StaticResource WhiteBackgroundBrush}" />
            <Setter Property="FocusVisualStyle" Value="{x:Null}" />
            <Setter Property="Padding" Value="5" />
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="Button">
                        <Border Background="{TemplateBinding Background}"
                                BorderBrush="{TemplateBinding BorderBrush}"
                                BorderThickness="{TemplateBinding BorderThickness}">
                            <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center" />
                        </Border>
                        <ControlTemplate.Triggers>
                            <Trigger Property="IsMouseOver" Value="True">
                                <Setter Property="Background" Value="{StaticResource LightGrayMouseOverBrush}" />
                                <Setter Property="Foreground" Value="{StaticResource LightForegroundBrush}" />
                            </Trigger>
                            <Trigger Property="IsPressed" Value="True">
                                <Setter Property="Background" Value="#a8a8a8" />
                                <Setter Property="Foreground" Value="{StaticResource LightForegroundBrush}" />
                            </Trigger>
                        </ControlTemplate.Triggers>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>

        <!-- ReSharper disable once Xaml.RedundantResource -->
        <Style x:Key="BottomButtonDark" TargetType="Button" BasedOn="{StaticResource BaseButtonStyle}">
            <Setter Property="Background" Value="{StaticResource DarkerGrayBackgroundBrush}" />
            <Setter Property="FocusVisualStyle" Value="{x:Null}" />
            <Setter Property="Padding" Value="5" />
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="Button">
                        <Border Background="{TemplateBinding Background}"
                                BorderBrush="{TemplateBinding BorderBrush}"
                                BorderThickness="{TemplateBinding BorderThickness}">
                            <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center" />
                        </Border>
                        <ControlTemplate.Triggers>
                            <Trigger Property="IsMouseOver" Value="True">
                                <Setter Property="Background" Value="#434343" />
                                <Setter Property="Foreground" Value="{StaticResource DarkForegroundBrush}" />
                            </Trigger>
                            <Trigger Property="IsPressed" Value="True">
                                <Setter Property="Background" Value="#313131" />
                                <Setter Property="Foreground" Value="{StaticResource DarkForegroundBrush}" />
                            </Trigger>
                        </ControlTemplate.Triggers>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>

        <!-- ************************ Styles pour les onglets ************************ -->
        <Style x:Key="LightOnglet" TargetType="TabItem">
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="TabItem">
                        <Grid>
                            <Border x:Name="Border" Background="White" BorderBrush="#b8b8b8" BorderThickness="1,1,1,0"
                                    CornerRadius="0" Padding="10,5">
                                <Grid>

                                    <ContentPresenter x:Name="ContentSite"
                                                      VerticalAlignment="Center"
                                                      HorizontalAlignment="Center"
                                                      ContentSource="Header"
                                                      RecognizesAccessKey="True" />
                                </Grid>
                            </Border>
                        </Grid>
                        <ControlTemplate.Triggers>
                            <Trigger Property="IsSelected" Value="True">
                                <Setter TargetName="Border" Property="Panel.ZIndex" Value="1" />
                                <Setter TargetName="Border" Property="Background" Value="White" />
                                <Setter TargetName="Border" Property="BorderThickness" Value="1,1,1,0" />
                                <!-- Supprimer la bordure en bas -->
                            </Trigger>
                            <Trigger Property="IsSelected" Value="False">
                                <Setter TargetName="Border" Property="Background" Value="#F5F5F5" />
                                <Setter TargetName="Border" Property="BorderBrush" Value="Transparent" />
                                <Setter TargetName="Border" Property="BorderThickness" Value="1,1,1,1" />
                                <!-- Rétablir la bordure en bas -->
                            </Trigger>
                            <MultiTrigger>
                                <MultiTrigger.Conditions>
                                    <Condition Property="IsMouseOver" Value="True" />
                                    <Condition Property="IsSelected" Value="False" />
                                </MultiTrigger.Conditions>
                                <Setter TargetName="Border" Property="Background" Value="#D3D3D3" />
                                <Setter TargetName="Border" Property="BorderBrush" Value="#D3D3D3" />
                            </MultiTrigger>
                        </ControlTemplate.Triggers>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>

        <!-- ReSharper disable once Xaml.RedundantResource -->
        <Style x:Key="DarkOnglet" TargetType="TabItem">
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="TabItem">
                        <Grid>
                            <Border x:Name="Border" Background="#262626" BorderBrush="#434343"
                                    BorderThickness="1,1,1,0" CornerRadius="0" Padding="10,5">
                                <Grid>

                                    <ContentPresenter x:Name="ContentSite"
                                                      VerticalAlignment="Center"
                                                      HorizontalAlignment="Center"
                                                      ContentSource="Header"
                                                      RecognizesAccessKey="True" />
                                </Grid>
                            </Border>
                        </Grid>
                        <ControlTemplate.Triggers>
                            <Trigger Property="IsSelected" Value="True">
                                <Setter TargetName="Border" Property="Panel.ZIndex" Value="1" />
                                <Setter TargetName="Border" Property="Background" Value="#262626" />
                                <Setter TargetName="Border" Property="BorderThickness" Value="1,1,1,0" />
                            </Trigger>
                            <Trigger Property="IsSelected" Value="False">
                                <Setter TargetName="Border" Property="Background" Value="#313131" />
                                <Setter TargetName="Border" Property="BorderBrush" Value="Transparent" />
                                <Setter TargetName="Border" Property="BorderThickness" Value="1,1,1,1" />
                            </Trigger>
                            <MultiTrigger>
                                <MultiTrigger.Conditions>
                                    <Condition Property="IsMouseOver" Value="True" />
                                    <Condition Property="IsSelected" Value="False" />
                                </MultiTrigger.Conditions>
                                <Setter TargetName="Border" Property="Background" Value="#404040" />
                                <Setter TargetName="Border" Property="BorderBrush" Value="#404040" />
                            </MultiTrigger>
                        </ControlTemplate.Triggers>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>


        <!-- ************************ Styles pour le slider ************************ -->
        <Style x:Key="LightSliderThumbStyle" TargetType="Thumb">
            <Setter Property="Width" Value="15" />
            <Setter Property="Height" Value="15" />
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="Thumb">
                        <Canvas>
                            <Ellipse Fill="#3E70B4"
                                     Stroke="{StaticResource WhiteBrush}"
                                     Width="15"
                                     Height="15" />
                            <TextBlock x:Name="ValueText"
                                       Text="{Binding Path=Value, RelativeSource={RelativeSource AncestorType=Slider}, StringFormat={}{0:F0}%}"
                                       HorizontalAlignment="Center"
                                       VerticalAlignment="Center"
                                       Foreground="#000000"
                                       Canvas.Left="-10" Canvas.Top="-25"
                                       Width="40" TextAlignment="Center" />
                        </Canvas>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>

        <Style x:Key="TransparentRepeatButtonStyle" TargetType="RepeatButton">
            <Setter Property="Background" Value="{StaticResource NoColorBrush}" />
            <Setter Property="BorderBrush" Value="{StaticResource NoColorBrush}" />
            <Setter Property="BorderThickness" Value="0" />
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="RepeatButton">
                        <Border BorderBrush="{TemplateBinding BorderBrush}"
                                BorderThickness="{TemplateBinding BorderThickness}"
                                Background="{TemplateBinding Background}">
                            <ContentPresenter HorizontalAlignment="Stretch" VerticalAlignment="Stretch" />
                        </Border>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
            <Style.Triggers>
                <Trigger Property="IsMouseOver" Value="True">
                    <Setter Property="Background" Value="{StaticResource NoColorBrush}" />
                    <Setter Property="BorderBrush" Value="{StaticResource NoColorBrush}" />
                </Trigger>
            </Style.Triggers>
        </Style>

        <Style x:Key="LightSlider" TargetType="Slider">
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="Slider">
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="*" />
                                <RowDefinition Height="Auto" />
                            </Grid.RowDefinitions>

                            <Grid Grid.Row="0" VerticalAlignment="Center">
                                <Rectangle x:Name="PART_TrackBackground"
                                           Fill="{StaticResource LightGrayBorderBrush}"
                                           Height="2"
                                           VerticalAlignment="Center" />

                                <RepeatButton Command="{x:Static Slider.DecreaseLarge}"
                                              Style="{StaticResource TransparentRepeatButtonStyle}"
                                              Width="{Binding ElementName=ScaleSlider, Path=ActualWidth}"
                                              Height="10"
                                              HorizontalAlignment="Left"
                                              Click="OnSliderClick" />
                                <Track x:Name="PART_Track">
                                    <Track.DecreaseRepeatButton>
                                        <RepeatButton Command="{x:Static Slider.DecreaseLarge}"
                                                      Style="{x:Null}"
                                                      Width="0"
                                                      Height="0" />
                                    </Track.DecreaseRepeatButton>
                                    <Track.Thumb>
                                        <Thumb Style="{StaticResource LightSliderThumbStyle}" />
                                    </Track.Thumb>
                                    <Track.IncreaseRepeatButton>
                                        <RepeatButton Command="{x:Static Slider.IncreaseLarge}"
                                                      Style="{x:Null}"
                                                      Width="0"
                                                      Height="0" />
                                    </Track.IncreaseRepeatButton>
                                </Track>
                            </Grid>

                            <TickBar x:Name="BottomTick"
                                     Fill="DimGray"
                                     Placement="Bottom"
                                     Height="4"
                                     VerticalAlignment="Top"
                                     Grid.Row="1"
                                     TickFrequency="25" />
                        </Grid>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>


        <Style x:Key="DarkSliderThumbStyle" TargetType="Thumb">
            <Setter Property="Width" Value="15" />
            <Setter Property="Height" Value="15" />
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="Thumb">
                        <Canvas>
                            <Ellipse Fill="#3E70B4"
                                     Stroke="#E3DED4"
                                     Width="15"
                                     Height="15" />
                            <TextBlock x:Name="ValueText"
                                       Text="{Binding Path=Value, RelativeSource={RelativeSource AncestorType=Slider}, StringFormat={}{0:F0}%}"
                                       HorizontalAlignment="Center"
                                       VerticalAlignment="Center"
                                       Foreground="#E3DED4"
                                       Canvas.Left="-10" Canvas.Top="-25"
                                       Width="40" TextAlignment="Center" />
                        </Canvas>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>
        
        <!-- ReSharper disable once Xaml.RedundantResource -->
        <Style x:Key="DarkSlider" TargetType="Slider">
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="Slider">
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="*" />
                                <RowDefinition Height="Auto" />
                            </Grid.RowDefinitions>

                            <Grid Grid.Row="0" VerticalAlignment="Center">
                                <Rectangle x:Name="PART_TrackBackground"
                                           Fill="#434343"
                                           Height="2"
                                           VerticalAlignment="Center" />
                                <RepeatButton Command="{x:Static Slider.DecreaseLarge}"
                                              Style="{StaticResource TransparentRepeatButtonStyle}"
                                              Width="{Binding ElementName=ScaleSlider, Path=ActualWidth}"
                                              Height="10"
                                              HorizontalAlignment="Left"
                                              Click="OnSliderClick" />
                                <Track x:Name="PART_Track">
                                    <Track.DecreaseRepeatButton>
                                        <RepeatButton Command="{x:Static Slider.DecreaseLarge}"
                                                      Style="{x:Null}"
                                                      Width="0"
                                                      Height="0" />
                                    </Track.DecreaseRepeatButton>
                                    <Track.Thumb>
                                        <Thumb Style="{StaticResource DarkSliderThumbStyle}" />
                                    </Track.Thumb>
                                    <Track.IncreaseRepeatButton>
                                        <RepeatButton Command="{x:Static Slider.IncreaseLarge}"
                                                      Style="{x:Null}"
                                                      Width="0"
                                                      Height="0" />
                                    </Track.IncreaseRepeatButton>
                                </Track>
                            </Grid>

                            <TickBar x:Name="BottomTick"
                                     Fill="DimGray"
                                     Placement="Bottom"
                                     Height="4"
                                     VerticalAlignment="Top"
                                     Grid.Row="1"
                                     TickFrequency="25" />
                        </Grid>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>

    </Window.Resources>

    <Border BorderBrush="{StaticResource KnxGreenBrush}"
            BorderThickness="0" x:Name="SettingsWindowBorder">
        <Grid x:Name="MainGrid">

            <Grid.RowDefinitions>
                <RowDefinition Height="60" /> <!-- En-tête (header) -->
                <RowDefinition Height="*" /> <!-- Contenu de la fenêtre (corps) -->
                <RowDefinition Height="50" /> <!-- Footer avec les boutons save/cancel -->
            </Grid.RowDefinitions>

            <!-- En-tête de la fenêtre -->
            <Viewbox Grid.Row="0" HorizontalAlignment="Left" MouseLeftButtonDown="Header_MouseLeftButtonDown">
                <StackPanel Orientation="Vertical" Background="Transparent" Width="500" Margin="0">
                    <!-- Icône + titre paramètres -->
                    <StackPanel Orientation="Horizontal">
                        <Image Height="30" Width="30" Margin="14,10,5,0" VerticalAlignment="Center">
                            <Image.Source>
