                                               Margin="10,10,0,0" />

                                    <TextBlock x:Name="ThemeTextBox" Margin="10,0,10,2" />
                                    <ComboBox x:Name="ThemeComboBox" Margin="10,0,10,10"
                                              Style="{StaticResource LightComboBoxStyle}">
                                        <ComboBoxItem x:Name="LightThemeComboBoxItem" IsSelected="True" Style="{DynamicResource CurrentComboBoxItemContainerStyle}"/>
                                        <ComboBoxItem x:Name="DarkThemeComboBoxItem" Style="{DynamicResource CurrentComboBoxItemContainerStyle}"/>
                                    </ComboBox>

                                    <TextBlock x:Name="AppLanguageTextBlock" Margin="10,0,10,2" />
                                    <ComboBox x:Name="AppLanguageComboBox" Margin="10,0,10,20"
                                              Style="{StaticResource LightComboBoxStyle}">
                                        <ComboBoxItem Content="AR - العربية"           Style="{DynamicResource CurrentComboBoxItemContainerStyle}"/>
                                        <ComboBoxItem Content="BG - Български"        Style="{DynamicResource CurrentComboBoxItemContainerStyle}"/>
                                        <ComboBoxItem Content="CS - Čeština"          Style="{DynamicResource CurrentComboBoxItemContainerStyle}"/>
                                        <ComboBoxItem Content="DA - Dansk"            Style="{DynamicResource CurrentComboBoxItemContainerStyle}" />
                                        <ComboBoxItem Content="DE - Deutsch"          Style="{DynamicResource CurrentComboBoxItemContainerStyle}"/>
                                        <ComboBoxItem Content="EL - Ελληνικά"         Style="{DynamicResource CurrentComboBoxItemContainerStyle}"/>
                                        <ComboBoxItem Content="EN - English"          Style="{DynamicResource CurrentComboBoxItemContainerStyle}"/>
                                        <ComboBoxItem Content="ES - Español"          Style="{DynamicResource CurrentComboBoxItemContainerStyle}"/>
                                        <ComboBoxItem Content="ET - Eesti"            Style="{DynamicResource CurrentComboBoxItemContainerStyle}"/>
                                        <ComboBoxItem Content="FI - Suomi"            Style="{DynamicResource CurrentComboBoxItemContainerStyle}"/>
                                        <ComboBoxItem x:Name="FrAppLanguageComboBoxItem"
                                                      Content="FR - Français (par défaut)"
                                                      IsSelected="True" FontWeight="Bold" 
                                                      Style="{DynamicResource CurrentComboBoxItemContainerStyle}"/>
                                        <ComboBoxItem Content="HU - Magyar"           Style="{DynamicResource CurrentComboBoxItemContainerStyle}"/>
                                        <ComboBoxItem Content="ID - Bahasa Indonesia" Style="{DynamicResource CurrentComboBoxItemContainerStyle}"/>
                                        <ComboBoxItem Content="IT - Italiano"         Style="{DynamicResource CurrentComboBoxItemContainerStyle}"/>
                                        <ComboBoxItem Content="JA - 日本語"            Style="{DynamicResource CurrentComboBoxItemContainerStyle}"/>
                                        <ComboBoxItem Content="KO - 한국어"             Style="{DynamicResource CurrentComboBoxItemContainerStyle}"/>
                                        <ComboBoxItem Content="LT - Lietuvių"         Style="{DynamicResource CurrentComboBoxItemContainerStyle}"/>
                                        <ComboBoxItem Content="LV - Latviešu"         Style="{DynamicResource CurrentComboBoxItemContainerStyle}"/>
                                        <ComboBoxItem Content="NB - Norsk Bokmål"     Style="{DynamicResource CurrentComboBoxItemContainerStyle}"/>
                                        <ComboBoxItem Content="NL - Nederlands"       Style="{DynamicResource CurrentComboBoxItemContainerStyle}" />
                                        <ComboBoxItem Content="PL - Polski"           Style="{DynamicResource CurrentComboBoxItemContainerStyle}" />
                                        <ComboBoxItem Content="PT - Português"        Style="{DynamicResource CurrentComboBoxItemContainerStyle}" />
                                        <ComboBoxItem Content="RO - Română"           Style="{DynamicResource CurrentComboBoxItemContainerStyle}" />
                                        <ComboBoxItem Content="RU - Русский"          Style="{DynamicResource CurrentComboBoxItemContainerStyle}"/>
                                        <ComboBoxItem Content="SK - Slovenský"        Style="{DynamicResource CurrentComboBoxItemContainerStyle}" />
                                        <ComboBoxItem Content="SL - Slovenščina"      Style="{DynamicResource CurrentComboBoxItemContainerStyle}"/>
                                        <ComboBoxItem Content="SV - Svenska"          Style="{DynamicResource CurrentComboBoxItemContainerStyle}"/>
                                        <ComboBoxItem Content="TR - Türkçe"           Style="{DynamicResource CurrentComboBoxItemContainerStyle}" />
                                        <ComboBoxItem Content="UK - Українська"       Style="{DynamicResource CurrentComboBoxItemContainerStyle}" />
                                        <ComboBoxItem Content="ZH - 中文"              Style="{DynamicResource CurrentComboBoxItemContainerStyle}"/>
                                    </ComboBox>

                                    <TextBlock x:Name="ScalingText"
                                               Margin="10,0,10,20" />
                                    <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
                                        <Slider Style="{StaticResource LightSlider}"
                                                Name="ScaleSlider"
                                                Minimum="50"
                                                Maximum="300"
                                                Value="100"
                                                IsSnapToTickEnabled="True"
                                                TickPlacement="BottomRight"
                                                TickFrequency="25"
                                                Width="400"
                                                HorizontalAlignment="Center"
                                                VerticalAlignment="Center"
                                                Margin="0,10,0,10" />
                                        <Grid Width="400" HorizontalAlignment="Center">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="Auto" />
                                                <ColumnDefinition Width="*" />
                                                <ColumnDefinition Width="Auto" />
                                            </Grid.ColumnDefinitions>
                                            <TextBlock Grid.Column="0"
                                                       Text="{Binding ElementName=ScaleSlider, Path=Minimum, StringFormat={}{0:F0}%}"
                                                       HorizontalAlignment="Left"
                                                       VerticalAlignment="Center"
                                                       Margin="0,0,10,10" />
                                            <TextBlock Grid.Column="2"
                                                       Text="{Binding ElementName=ScaleSlider, Path=Maximum, StringFormat={}{0:F0}%}"
                                                       HorizontalAlignment="Right"
                                                       VerticalAlignment="Center"
                                                       Margin="10,0,0,10" />

                                        </Grid>
                                    </StackPanel>
                                </StackPanel>
                            </ScrollViewer>
                        </TabItem>

