                        </Image>
                        <TextBlock x:Name="SettingsWindowTopTitle" FontFamily="Aptos" FontWeight="Bold" FontSize="22"
                                   VerticalAlignment="Center" Margin="0, 12, 0, 0" />
                    </StackPanel>

