                                </ScrollViewer>
                            </Grid>
                        </TabItem>

                    </TabControl>

                </Border>
            </Grid>


            <!-- Partie basse de la fenêtre (boutons "sauvegarder" et "annuler") -->
            <StackPanel x:Name="SettingsWindowFooter" Grid.Row="2" Orientation="Vertical" Margin="0,5,0,0"
                        Background="White">

                <!-- Trait de séparation -->
                <Path x:Name="FooterPath" Stroke="#D7D7D7" StrokeThickness="2">
                    <Path.Data>
                        <LineGeometry StartPoint="0,1" EndPoint="500,1" />
                    </Path.Data>
                </Path>

                <!-- Bouton de sauvegarde et d'annulation -->
                <StackPanel Orientation="Horizontal" HorizontalAlignment="Right" Margin="0,5,10,5">

                    <!-- Bouton save -->
                    <Button x:Name="SaveButton" Margin="0,0,15,0" Height="32"
                            Style="{StaticResource BottomButtonLight}"
                            Click="SaveButtonClick" Visibility="Visible">
                        <StackPanel Orientation="Horizontal">
                            <Image x:Name="SaveSettImage"
                                   Width="20" Height="20" Margin="5,0,5,0" VerticalAlignment="Center"
                                   Source="{StaticResource CheckmarkLight}">
                            </Image>
                            <TextBlock x:Name="SaveButtonText" FontWeight="Regular" VerticalAlignment="Center"
                                       FontFamily="Aptos" FontSize="16"
                                       HorizontalAlignment="Center" Margin="5,0,5,0" />
                        </StackPanel>
                    </Button>

                    <!-- Bouton cancel -->
                    <Button x:Name="CancelButton" Margin="15,0,0,0" Height="32"
                            Style="{StaticResource BottomButtonLight}"
                            Click="CancelButtonClick" Visibility="Collapsed">
                        <StackPanel Orientation="Horizontal" Visibility="Collapsed">
                            <Image x:Name="CancelButtonImage"
                                   Width="16" Height="16" Margin="5,0,5,0" VerticalAlignment="Center"
                                   Source="{StaticResource CrossmarkLight}"/>
                                
                            <TextBlock x:Name="CancelButtonText" FontWeight="Regular" VerticalAlignment="Center"
                                       FontFamily="Aptos" FontSize="16"
                                       HorizontalAlignment="Center" Margin="5,0,5,0" />
                        </StackPanel>
                    </Button>

